<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<h1>Edit User</h1>

<%= form_for(@user) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br />
	<%= f.label :email %>
	<%= f.text_field :email %><br />
	<%= f.label :password %>
	<%= f.password_field :password %><br >
	<%= f.submit :update %>
<% end %>
